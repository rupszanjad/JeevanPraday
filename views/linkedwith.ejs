<html>
<title>jivan-praday|Collaboratives</title>

<head>
  
    <LINK rel="stylesheet" href="../stylesheets/user.css">
    <link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>





</head>

<style>
.navbar #tabmenu .custom-nav .nav-item a {
    color: #fff;
    padding: 10px;
}

.navbar #tabmenu .custom-nav .nav-item a:hover {
    color: #F0AAB1;
}

</style>

<body>

    <!-- start navigation -->
    <div>
        <nav class="navbar navbar-expand-md navbar-dark bg-danger pl-5 fixed-top" style="background-color: rgb(245, 10, 10);">
            <img src="../images/logo.jpg" class="image-fluid" style="height:80px; width:80px; border-radius: 80px;" />
            <a href="/" class="navbar-portal" style="text-decoration: none; color:white; font-size:40px;">जीवनप्रदाय <br><h6 style="margin-left: 0px; color: black; font: italic;">Innovation Responsibility And Humanism</h6></a>
            <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#tabmenu">
          <span class="navbar-toggler-icon"></span>
      </button>
            <div class="collapse navbar-collapse" id="tabmenu">
                <ul class="navbar-nav pl-5 custom-nav" style="margin-left:6%; font-size:20px; color:rgb(8, 8, 7);">
                    <li class="nav-item"><a href="/" class="nav-link">Home</a> </li>

                </ul>
            </div>
        </nav>
    </div>
    <!-- end navigation -->
    <div class="container">
        <div class="row no-gutters">
            <!-- FAQ's section -->

            <div class="col-lg-12">
                <div>


                    <div class="container-fluid">
                        <div style="margin-top:100px;">
                            <div class="col-md-6 thumb"></div>
                            <div class="jumbotron">
                                <h5>We are providing you list of collaborated Blood Banks and NGO's.<br>You can directly contact the following Blood Banks for any kind of help.</h5>
                            </div>

                                <form action="/linkedwith" method="POST">
                                <input placeholder="enter district" name="dist"><br>
                                <button class="btn btn-primary" onclick="window.location.href='/linkedwith';">Search</button>
                            </form>
                            </br>
                        </div>
                    </div>
                    <div class="data-tables" style="margin-left:50px; margin-right: 50px;">
                        <table id="dataTable" class="text-center">
                            <thead class="bg-light text-capitalize">
                                <tr>
                                    <th hidden>Ref ID</th>
                                    <th>Collaborative</th>
                                    <th>Name</th>
                                    <th>District</th>
                                    <th>Address</th>
                                    <th>Contact Details</th>

                                </tr>
                            </thead>
                            <tbody id="tableBody">
                                <% if(records.length>0){
                        records.forEach(function(row){
                            %>

                                    <tr>
                                        <td>
                                            <%= row.bcordinate %>
                                        </td>
                                        <td>
                                            <%= row.bname %>
                                        </td>
                                        <td>
                                            <%= row.bdist %>
                                        </td>
                                        <td>
                                            <%= row.bcity %>
                                        </td>
                                        <td>
                                            <%= row.bcontact %>
                                        </td>
                                    </tr>

                                    <% }) } %>
                            </tbody>
                        </table>



                        <div div class="row " style="margin-top:5px; color: blue; font-size: larger; font-weight: bold;">
                            <% if (pages > 0) { %>
                                <ul class="pagination text-center">
                                    <% if (current == 1) { %>
                                        <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>First</a></li>
                                        <% } else { %>
                                            <li><a href="/linkedwith/1">First</a></li>
                                            <% } %>
                                                <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                                                    <% if (i !== 1) { %>
                                                        <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>...</a></li>
                                                        <% } %>
                                                            <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                                                                <% if (i == current) { %>
                                                                    <li class="active">
                                                                        <a>
                                                                            <%= i %>
                                                                        </a>
                                                                    </li>
                                                                    <% } else { %>
                                                                        <li>
                                                                            <a href="/linkedwith/<%= i %>">
                                                                                <%= i %>
                                                                            </a>
                                                                        </li>
                                                                        <% } %>
                                                                            <% if (i == Number(current) + 4 && i < pages) { %>
                                                                                <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>...</a></li>
                                                                                <% } %>
                                                                                    <% } %>
                                                                                        <% if (current == pages) { %>
                                                                                            <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>Last</a></li>
                                                                                            <% } else { %>
                                                                                                <li><a href="/linkedwith/<%= pages %>">Last</a></li>
                                                                                                <% } %>
                                </ul>
                                <% } %>


                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>



 <!-- Start Footer-->
 <footer class="container-fluid text-black mt-5 " style="border-top: 3px solid #DC3545; background-color: rgb(240, 219, 219); margin-bottom:0px; height:100px;">
    <div class="container ">
        <!-- Start Footer Container -->
        <div class="row py-3 ">
            <!-- Start Footer Row -->
            <div class="col-md-6 ">
                <!-- Start Footer 1st Column -->
