    <!
    <!-- end navigation -->
    <div>
        <div class="container">
            <div class="row no-gutters">
                <!-- FAQ's section -->

                <div class="col-lg-12">

                    <div class="container-fluid">
                        <div class="row justify-content-center" style="margin-top:100px;">

                            <div class="col-md-6 thumb">

                                <h2>Camp Check list</h2>
                                </br>
                            </div>

                        </div>
                    </div>
                    <div class="data-tables">
                        <table id="dataTable" class="text-center">
                            <thead class="bg-light text-capitalize">
                                <tr>
                                    <th hidden>Ref ID</th>
                                    <th>Organiser </th>
                                    <th>District</th>
                                    <th>Venue</th>
                                    <th>Date</th>
                                    <th>Time</th>
                                    <th>Contact Details</th>

                                    <th>Status</th>
                                    <th>Action</th>

                                </tr>
                            </thead>
                            <tbody id="tableBody">
                                <% if(records.length>0){
                        records.forEach(function(row){
                            %>

                                    <tr>
                                        <td>
                                            <%= row.organizer %>
                                        </td>
                                        <td>
                                            <%= row.district %>
                                        </td>
                                        <td>
                                            <%= row.venue %>
                                        </td>
                                        <td>
                                            <%= row.date %>
                                        </td>
                                        <td>
                                            <%= row.time %>
                                        </td>
                                        <td>
                                            <%= row.contact %>
                                        </td>
                                        <td>
                                            <%= row.status %>
                                        </td>
                                        <td><a class="btn btn-primary btn-xs" href="/edit/<%= row._id %>"><i class="fa fa-edit" aria-hidden="true"></i></a>
                                            <a class="btn btn-danger btn-xs" href="/delete/<%= row._id %>"><i class="fa fa-trash" aria-hidden="true"></i></a></td>

                                    </tr>

                                    <% }) } %>
                            </tbody>
                        </table>


                        <div div class="row " style="margin-top:5px; color: blue; font-size: larger; font-weight: bold;">
                            <% if (pages > 0) { %>
                                <ul class="pagination text-center">
                                    <% if (current == 1) { %>
                                        <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>First</a></li>
                                        <% } else { %>
                                            <li><a href="/showvalidcamps/1">First</a></li>
                                            <% } %>
                                                <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                                                    <% if (i !== 1) { %>
                                                        <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>...</a></li>
                                                        <% } %>
                                                            <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                                                                <% if (i == current) { %>
                                                                    <li class="active">
                                                                        <a>
                                                                            <%= i %>
                                                                        </a>
                                                                    </li>
                                                                    <% } else { %>
                                                                        <li>
                                                                            <a href="/showvalidcamps/<%= i %>">
                                                                                <%= i %>
                                                                            </a>
                                                                        </li>
                                                                        <% } %>
                                                                            <% if (i == Number(current) + 4 && i < pages) { %>
                                                                                <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>...</a></li>
                                                                                <% } %>
                                                                                    <% } %>
                                                                                        <% if (current == pages) { %>
                                                                                            <li class="disabled" style="color: rgba(17, 10, 10, 0.767);"><a>Last</a></li>
                                                                                            <% } else { %>
                                                                                                <li><a href="/showvalidcamps/<%= pages %>">Last</a></li>
                                                                                                <% } %>
                                </ul>
                                <% } %>


                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

